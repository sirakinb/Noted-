{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "sts:GetFederationToken"
      ],
      "Resource": [
        "arn:aws:s3:::noted-audio-files",
        "arn:aws:s3:::noted-audio-files/*"
      ]
    }
  ]
} 